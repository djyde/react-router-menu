{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "declaration": true,
    "rootDirs": [
      "src",
      "__story__"
    ],
    "outDir": "dist",
    "jsx": "react",
    "lib": [
      "es5",
      "es2015",
      "dom",
      "scripthost"
    ]
  },
  "exclude": [
    "__story__",
    "node_modules"
  ]
}